    <!-- Buttons =========================== -->
    <style name="FabButton" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="backgroundTint">@color/fab_primary_csl</item>
        <item name="tint">?colorOnPrimary</item>
    </style>

    <style name="FabButton.Secondary">
        <item name="backgroundTint">?colorSecondary</item>
        <item name="tint">?colorOnSecondary</item>
    </style>

    <style name="Button.IconOnly" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="iconTint">@color/button_csl</item>
        <item name="strokeColor">@color/button_csl</item>
        <item name="rippleColor">@color/grey_400</item>
        <item name="android:tint">@color/button_csl</item>
        <item name="iconSize">18dp</item>
    </style>

    <style name="Button.Action" parent="@style/Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:tint">@color/button_csl</item>
        <item name="tint">@color/button_csl</item>
        <item name="iconTint">@color/button_csl</item>
        <item name="android:textColor">@color/button_csl</item>
        <item name="strokeColor">@color/button_csl</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:layout_margin">8dp</item>
        <item name="rippleColor">@color/grey_400</item>
        <item name="iconSize">18dp</item>
    </style>

    <style name="Button.Solid" parent="@style/Widget.MaterialComponents.Button.Icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableTint">@color/button_fg_tint</item>
        <item name="iconTint">@color/white</item>
        <item name="tint">?colorPrimary</item>
        <item name="android:elevation">@dimen/button_elevation</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:layout_margin">8dp</item>
        <item name="rippleColor">?colorPrimaryVariant</item>
        <item name="iconSize">18dp</item>
    </style>
    <!-- end Buttons =========================== -->

    <!-- Full screen =========================== -->
    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/grey_800</item>
    </style>
    <!-- end Full screen =========================== -->


</resources>
