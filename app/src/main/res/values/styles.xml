    <style name="Toolbar" parent="@style/Widget.MaterialComponents.Toolbar">
        <item name="fontFamily">?headlineFont</item>
        <item name="android:fontFamily">?headlineFont</item>
    </style>

    <style name="Cuer" />

    <style name="Cuer.AppBar" />

    <style name="Cuer.AppBar.OverFlow">
        <item name="android:src">@drawable/ic_more_vert_black</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:tint">@color/grey_500</item>
        <item name="android:elevation">4dp</item>
    </style>

    <style name="Cuer.MaterialAlertDialog" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="alertDialogStyle">@style/Cuer.MaterialAlertDialog.Window</item>
        <item name="buttonBarPositiveButtonStyle">@style/Cuer.MaterialAlertDialog.Button.Positive</item>
        <item name="buttonBarNegativeButtonStyle">@style/Cuer.MaterialAlertDialog.Button</item>
        <item name="buttonBarNeutralButtonStyle">@style/Cuer.MaterialAlertDialog.Button</item>
        <item name="materialAlertDialogTitleTextStyle">
            @style/Cuer.MaterialAlertDialog.App.Title.Text
        </item>
    </style>

    <style name="Cuer.MaterialAlertDialog.Window" parent="MaterialAlertDialog.MaterialComponents">
        <item name="shapeAppearance">@style/ShapeAppearance.Medium</item>
    </style>

    <style name="Cuer.MaterialAlertDialog.App.Title.Text" parent="MaterialAlertDialog.MaterialComponents.Title.Text">
        <item name="android:textSize">24sp</item>
        <item name="fontFamily">?headlineFont</item>
        <item name="android:fontFamily">?headlineFont</item>
    </style>

    <style name="Cuer.MaterialAlertDialog.Button" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:tint">@color/button_csl</item>
        <item name="iconTint">@color/button_csl</item>
        <item name="android:textColor">@color/button_csl</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="rippleColor">@color/grey_500</item>
        <item name="iconSize">18dp</item>
    </style>

    <!-- doesn't show border -->
    <style name="Cuer.MaterialAlertDialog.Button.Positive">
        <item name="strokeColor">@color/button_primary_csl</item>
        <item name="android:tint">@color/button_primary_csl</item>
        <item name="iconTint">@color/button_primary_csl</item>
        <item name="android:textColor">@color/button_primary_csl</item>
        <item name="rippleColor">@color/primary_variant</item>
    </style>

    <style name="Cuer.MaterialAlertDialog.Item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:drawableTint">@color/text_primary</item>
        <item name="android:padding">16dp</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:letterSpacing">?headlineFontLetterSpacing</item>
        <item name="android:textScaleX">?headlineFontXScaling</item>
    </style>

    <!-- Headers =========================== -->
    <style name="Header"/>

    <style name="Header.InfoText">
        <item name="android:textColor">?colorOnSurface</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingVertical">4dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:textAppearance">@style/TextAppearance.MaterialComponents.Subtitle1</item>
        <item name="drawableTint">?colorOnSurface</item>
        <item name="android:drawableTint">?colorOnSurface</item>
        <item name="android:drawablePadding">4dp</item>
    </style>

    <style name="Header.InfoIcon">
        <item name="tint">?colorOnSurface</item>
        <item name="android:tint">?colorOnSurface</item>
        <item name="android:layout_width">@dimen/flag_size</item>
        <item name="android:layout_height">@dimen/flag_size</item>
        <item name="android:paddingVertical">4dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:layout_marginTop">4dp</item>
    </style>

    <style name="Header.Button" parent="@style/Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:tint">@color/button_csl</item>
        <item name="iconTint">@color/button_csl</item>
        <item name="android:textColor">@color/button_csl</item>
        <item name="strokeColor">@color/button_csl</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginVertical">8dp</item>
        <item name="rippleColor">@color/grey_400</item>
        <item name="iconSize">18dp</item>
    </style>

    <style name="Header.Button.Solid" parent="@style/Widget.MaterialComponents.Button.Icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableTint">@color/button_fg_tint</item>
        <item name="iconTint">@color/white</item>
        <item name="tint">?colorPrimary</item>
        <item name="android:elevation">@dimen/button_elevation</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginVertical">8dp</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="rippleColor">?colorPrimaryVariant</item>
        <item name="iconSize">18dp</item>
    </style>
    <!-- end Headers =========================== -->

    <!-- ItemView =========================== -->
    <style name="ItemView"/>

    <style name="ItemView.Container">
        <item name="android:layout_margin">@dimen/list_item_margin</item>
        <item name="android:background">@drawable/ic_ripple_item_background</item>
        <item name="android:elevation">@dimen/list_item_elevation</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/itemview_icon_height</item>
    </style>

    <style name="ItemView.Container.Playlist">
        <item name="android:layout_margin">@dimen/list_item_margin</item>
        <item name="android:background">@drawable/ic_ripple_item_background</item>
        <item name="android:elevation">@dimen/list_item_elevation</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/playlist_row_icon_height</item>
    </style>

    <style name="ItemView.Background">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">?colorOnPrimary</item>
        <item name="android:textSize">16sp</item>
        <item name="tint">?colorOnPrimary</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:drawableTint">?colorOnPrimary</item>
        <item name="android:alpha">0</item>
        <item name="android:padding">@dimen/list_item_swipe_margin</item>
    </style>

    <style name="ItemView.Background.Right">
        <item name="android:gravity">left|center_vertical</item>

    </style>

    <style name="ItemView.Background.Left">
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="ItemView.Thumb">
        <item name="android:padding">0dp</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="ItemView.Thumb.Row">
        <item name="android:layout_width">@dimen/itemview_icon_width</item>
        <item name="android:layout_height">@dimen/itemview_icon_height</item>
        <item name="android:background">@color/surface</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Item.Icon</item>
    </style>

    <style name="ItemView.Thumb.Playlist">
        <item name="android:layout_width">@dimen/playlist_row_icon_width</item>
        <item name="android:layout_height">@dimen/playlist_row_icon_height</item>
        <item name="android:background">@color/surface</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Item.Icon</item>
    </style>

    <style name="ItemView.Thumb.InfoText">
        <item name="android:background">@color/black_transparent_background</item>
        <item name="android:padding">4dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="ItemView.TopText" parent="@style/TextAppearance.MaterialComponents.Body1">
        <item name="android:textSize">@dimen/list_item_top_text_size</item>
        <item name="android:textAppearance">@style/TextAppearance.App.Subtitle2</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:lineSpacingExtra">-4dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">3</item>
        <item name="android:letterSpacing">?headlineFontLetterSpacing</item>
        <item name="android:textScaleX">?headlineFontXScaling</item>
    </style>

    <style name="ItemView.BottomText" parent="@style/TextAppearance.AppCompat.Small">
        <item name="android:textSize">@dimen/list_item_bottom_text_size</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:textColor">?android:attr/textColorSecondary</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="ItemView.OverflowButtonClick">
        <item name="android:layout_width">64dp</item>
        <item name="android:background">?selectableItemBackgroundBorderless</item>
    </style>

    <style name="ItemView.OverflowButton">
        <item name="android:layout_width">16dp</item>
        <item name="tint">@color/grey_300</item>
        <item name="android:src">@drawable/ic_more_vert_black</item>
        <item name="android:scaleType">center</item>
    </style>

    <style name="ItemView.CheckIcon">
        <item name="tint">?colorCheckIcon</item>
        <item name="android:src">@drawable/ic_item_tick_white</item>
        <item name="android:visibility">visible</item><!-- todo gone -->
    </style>

    <style name="ItemView.CheckIcon.Row">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
    </style>

    <style name="ContextMenu" parent="Widget.AppCompat.PopupMenu">
        <item name="android:textColor">?contextMenuItemColor</item>
        <item name="android:iconTint">?contextMenuItemColor</item>
    </style>
    <!-- end ItemView =========================== -->

    <!-- ItemTile =========================== -->
    <style name="ItemTile" />

    <style name="ItemTile.Root" >
        <item name="cardBackgroundColor">@color/surface</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Item.Icon</item>
        <item name="android:layout_width">@dimen/playlist_tile_size</item>
        <item name="android:layout_height">@dimen/playlist_tile_size</item>
    </style>

    <style name="ItemTile.Container">
        <item name="android:elevation">@dimen/list_item_elevation</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="ItemTile.Thumb">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="ItemTile.Title" parent="@style/TextAppearance.MaterialComponents.Body1">
        <item name="android:textSize">@dimen/list_item_top_text_size</item>
        <item name="android:textAppearance">@style/TextAppearance.App.Subtitle2</item>
        <item name="android:paddingStart">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingEnd">2dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:lineSpacingExtra">-4dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:letterSpacing">?headlineFontLetterSpacing</item>
        <item name="android:textScaleX">?headlineFontXScaling</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/black_transparent_background</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ItemTile.OverflowButtonClick">
        <item name="android:layout_width">@dimen/tile_overflow_button_size</item>
        <item name="android:layout_height">@dimen/tile_overflow_button_size</item>
        <item name="android:background">?selectableItemBackgroundBorderless</item>
    </style>
    <!-- end ItemTile =========================== -->
    <!-- ItemCard =========================== -->
    <style name="ItemCard"/>

    <style name="ItemCard.Root">
        <item name="cardBackgroundColor">@color/transparent</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Item.Icon</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ItemCard.Container">
        <item name="android:background">@color/surface</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:elevation">8dp</item>
    </style>

    <style name="ItemCard.OverflowButtonClick">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">?selectableItemBackgroundBorderless</item>
    </style>

    <style name="ItemCard.TopText" parent="@style/TextAppearance.MaterialComponents.Body1">
        <item name="android:textAppearance">@style/TextAppearance.App.Subtitle2</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:letterSpacing">?headlineFontLetterSpacing</item>
        <item name="android:textScaleX">?headlineFontXScaling</item>
